### ForEachAsync()

#!markdown

public static int Add(int x)
{ 
    return x + 1;
}

Func<int> funcAdd;
funcAdd = Add;

int x;
Func<int> funcAddNumber = () => x+1 ;

#!markdown

public async Task<bool> isEqualToOne(int x)
{
    return x.Equals(1);
}

Func<bool> funcIsEqualToOne = isEqualToOne();

#!markdown

var list = new List<int> { 1, 2, 3, 4, 5 };
list.ForEachAsync<int>(isEqualToOne(list[0]));

